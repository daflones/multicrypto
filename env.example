# =====================================================
# CONFIGURAÇÃO DBXBankPay
# =====================================================

# API Key do DBXBankPay (obter no dashboard)
VITE_DBXPAY_API_KEY=sua_api_key_aqui

# Secret para validação de webhooks (opcional, para segurança)
VITE_DBXPAY_WEBHOOK_SECRET=seu_webhook_secret_aqui

# URL base da aplicação (para webhooks)
VITE_APP_URL=http://localhost:3000

# =====================================================
# OUTRAS CONFIGURAÇÕES EXISTENTES
# =====================================================

# Supabase
VITE_SUPABASE_URL=sua_supabase_url
VITE_SUPABASE_ANON_KEY=sua_supabase_key

# Chaves PIX/Crypto existentes
VITE_PIX_KEY=sua_chave_pix
VITE_BEP20_KEY=seu_endereco_bep20
VITE_TRC20_KEY=seu_endereco_trc20
